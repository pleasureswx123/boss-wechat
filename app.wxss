.contain {
    width: 100%;
    overflow: hidden;
    display: flex;
    flex-direction: column;
    padding-top: constant(safe-area-inset-top);
    padding-left: constant(safe-area-inset-left);
    padding-right: constant(safe-area-inset-right);
    padding-bottom: constant(safe-area-inset-bottom);
}

@font-face {
  font-family: 'OPPOSans B';
  src: url('data:font/woff2;charset=utf-8;base64,') format('woff2'),
      url('data:font/woff;charset=utf-8;base64,') format('woff');
  font-weight: normal;
  font-style: normal;
  font-display: swap;
}


image {
    width: 100%;
    height: 100%;
    /* border-radius: 6rpx; */
}

/* 清除小程序button默认样式 */
button {
    margin: 0;
    padding: 0;
    text-decoration: none;
    font-weight: normal;
    -webkit-tap-highlight-color: transparent;
    overflow: hidden;
    color: #000000;
    background-color: #F8F8F8;
}

button::after {
    border: none;
}

button {
    background-color: #fff;
}

button {
    border-radius: 0;
}
page {
    width: 750rpx;
    font-family:Monospaced Number,Chinese Quote,-apple-system,BlinkMacSystemFont,Segoe UI,Roboto,PingFang SC,Hiragino Sans GB,Microsoft YaHei,Helvetica Neue,Helvetica,Arial,sans-serif!important;
}
.box {
    background-color: #F5F5F5;
    width: 750rpx;
    height: 100vh;
    overflow-x: hidden;
    overflow-y: hidden;
}
.line {
    width: 652rpx;
    height: 2rpx;
    background-color: #f5f5f5;
    margin: 0 auto;
}
.in_box {
    height: 100vh;
    overflow-y: scroll;
}
.default_btn {
    position: fixed;
    bottom: 100rpx;
    left: 50%;
    z-index: 9999;
    transform: translateX(-50%);
    width: 654rpx;
    height: 80rpx;
    background: linear-gradient(270deg, #ECECEC 0%, #D9D9D9 100%);
    border-radius: 12rpx;
    text-align: center;
    line-height: 80rpx;
    font-size: 32rpx;
    font-family: PingFangSC-Medium, PingFang SC;
    font-weight: 500;
    color: #FFFFFF;
}
.submit_btn {
    position: fixed;
    bottom: 70rpx;
    left: 50%;
    z-index: 99;
    transform: translateX(-50%);
    width: 654rpx;
    height: 80rpx;
    /* background: linear-gradient(270deg, #FF7979 0%, #F32E2E 100%); */
    background: linear-gradient(308deg, #FF7C44 0%, #FE0000 46%, #FF6E67 100%);
    border-radius: 12rpx;
    text-align: center;
    line-height: 80rpx;
    font-size: 32rpx;
    font-family: PingFangSC-Medium, PingFang SC;
    font-weight: 500;
    color: #FFFFFF;
}
.loadingBox{
  position: fixed;
  top:50%;
  z-index: 1000;
  left:50%;
  /* margin-left:-25px; */
  transform: translateX(-50%);
}
.loadingBox image{
  width: 50px;height: 50px;
}
/* 隐藏滚动条 */
::-webkit-scrollbar {
  display:none;
  width:0;
  height:0;
  color:transparent;
}

