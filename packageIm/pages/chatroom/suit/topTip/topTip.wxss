.showTip {
  height: 90rpx;
  background: #FEE8E8;
  display: flex;
  color: #1D2129;
  font-size: 24rpx;
  align-items: center;
  padding: 0 30rpx;
}

.wainTip {
  background: #FFE9DE !important;
  color: #FF5A00 !important;
}
.greyCss{
  background: #E2E2E2 !important;
  color: #9F9F9F !important;
}

.showTip image {
  width: 36rpx;
  height: 36rpx;
  margin-right: 16rpx;
}

.gui-space-between {
  justify-content: space-between;
}

.gui-flex {
  display: flex;
}

.see {
  height: 44rpx;
  line-height: 44rpx;
  background: linear-gradient( 308deg, #FF7C44 0%, #FE0000 46%, #FF6E67 100%);
border-radius: 22rpx;
  font-size: 22rpx;
  color: #fff;
  padding: 0 22rpx;
}
.see1{
  height: 44rpx;
  line-height: 44rpx;
  background: #9F9F9F;
border-radius: 22rpx;
  font-size: 22rpx;
  color: #fff;
  padding: 0 22rpx;
}

.edit {
  height: 36rpx;
  background: #FF5A00;
  border-radius: 18rpx;
  border: 2rpx solid #FF5A00;
  font-size: 24rpx;

  font-weight: 400;
  color: #FFFFFF;
  padding: 0 10px;
}

.wgCss {
  display: flex;
  justify-content: center;
  align-items: center;
}

.infoTip {
  height: 90rpx;
  background: #FFE9DE;
  backdrop-filter: blur(3px);
}

.infoTip .gui-flex {
  justify-content: space-between;
  width: 100%;
  align-items: center;
}

.infoTip .left image {
  width: 42rpx;
  height: 34rpx;
}

.infoTip .content {
  display: flex;
  flex-direction: column;
  flex: 1;
}

.infoTip .content .content-top {
  font-weight: 600;
  font-size: 28rpx;
  color: #FF5A00;
}

.infoTip .content .content-buttom {
  font-weight: 400;
  font-size: 24rpx;
  color: rgba(255, 90, 0, 0.7);
}


.infoTip .right {
  width: 112rpx;
  background: #FF5A00;
  border-radius: 18rpx;
  font-weight: 500;
  font-size: 24rpx;
  color: #FFFFFF;
  display: flex;
  align-items: center;
  justify-content: center;
}