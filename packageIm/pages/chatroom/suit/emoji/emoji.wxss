.emoji_list {
	display: none;
}

.showEmoji {
	width: 100%;
	height: 145px;
	background-color: #ffffff;
	padding-top: 10px;
	padding-left: 3%;
  display: block;
  overflow: hidden;
}

.emoji_list image,
.showEmoji image {
	width: 26px;
	height: 26px;
	margin: 5px 2%;
}

.emoji {
	width: 26px;
	height: 26px;
	margin: 0 0;
}

.emoji_item {
	display: flex;
	justify-content: space-around;
  margin-right: 20px;
}
