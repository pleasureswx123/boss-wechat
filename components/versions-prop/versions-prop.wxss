  width: 12rpx;
  height: 12rpx;
  border-radius: 50%;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: scale(0);
  transition: all .2s ease-in-out;
  display: inline-block;
}

.radio-checked {
  border-color: #FE0201;
  transform: scale(1);
  transition: all .2s ease-in-out;
}

.radio-checked .radio-inner {
  transform: scale(1);
  transition: all .2s ease-in-out;
  background: #FE0201;
  transform: translateY(-50%) translateX(-50%);
}


