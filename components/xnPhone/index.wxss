.popupcontact {
  height: 700rpx;
  background: #fff;
  /* background: linear-gradient( 269deg, #FFF1DE 0%, #FFF8F8 51%, #FFEAEA 100%), linear-gradient( 180deg, #FFFFFF 0%, rgba(253,253,253,0) 100%); */
}

.contactTitle {
  font-size: 44rpx;
  color: #1D2129;
  line-height: 150rpx;
  font-weight: 600;
  margin: 0 48rpx;
  position: relative;
  z-index: 100;
}

.contactCont {
  margin: 0 48rpx;
  position: relative;
}

.simage1 {
  /* height: 350rpx; */
  min-height: 400rpx;
  width: 654rpx;
  position: absolute;
  top: 0;
  z-index: 2;
  backdrop-filter: blur(5px);
}

.borderRadius {
  border-radius: 24rpx;
  background: rgba(255, 220, 220, 0.3);
}

.litem {
  width: 598rpx;
  height: 70rpx;
  background: #FFFFFF;
  border-radius: 12rpx;
  font-weight: 400;
  font-size: 30rpx;
  color: #1D2129;
  line-height: 42rpx;
  margin: 0 auto;
  margin-top: 22rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.tipCont {
  font-weight: 400;
  font-size: 26rpx;
  color: #1D2129;
  line-height: 42rpx;
  margin: 0 40rpx;
}

/* .tipCont:last-child{
  padding
} */

.litemList {
  padding-top: 5rpx;
  position: relative;
  z-index: 3;
}

.iconImage {
  width: 172rpx;
  height: 178rpx;
  position: absolute;
  right: 70rpx;
  top: -86rpx;
  z-index: 1;
}

.sclose {
  width: 30rpx;
  height: 30rpx;
  border-radius: 0;
  position: absolute;
  right: 30rpx;
  top: 0;
}

.scloseItem {
  right: -20rpx;
  top: 20px;
  position: absolute;
  width: 80rpx;
  height: 70rpx;
}

.itemPhone {
  display: flex;
  align-items: center;
  /* justify-content: space-between; */
  margin: 40rpx 40rpx 20rpx;
}

.itemPhone .phone {
  font-family: PingFangSC, PingFang SC;
  font-weight: 500;
  font-size: 36rpx;
  color: #1D2129;
}

.itemPhone image{
  width: 30rpx;
  height: 30rpx;
  margin-left: 10rpx;
}

.editBtn {
  font-weight: 500;
  font-size: 30rpx;
  color: #FE0201;
}

.phoneBtns {
  display: flex;
  margin-top: 70rpx;
}

.cancelBtn {
  width: 180rpx;
  height: 80rpx;
  background: #F1F1F1;
  border-radius: 12rpx;
  font-weight: 400;
  font-size: 36rpx;
  color: #666666;
  display: flex;
  justify-content: center;
  align-items: center;
}

.openBtn {
  width: 452rpx;
  height: 80rpx;
  /* background: linear-gradient(270deg, #FF7979 0%, #F32E2E 100%); */
  background: linear-gradient(308deg, #FF7C44 0%, #FE0000 46%, #FF6E67 100%);
  border-radius: 12rpx;
  font-weight: 500;
  font-size: 36rpx;
  color: #FFFFFF;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-left: 22rpx;
}

.topBg {
  width: 100%;
  height: 356rpx;
  position: absolute;
  top: 0;
  left: 0;
}

.topBg image {
  width: 100%;
  height: 100%;
}