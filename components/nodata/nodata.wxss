.no_data{
    position: relative;
    width: 100%;
    height: 200px;
    padding-top: 40%;
}
.no_data_center {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
    display: flex;
    flex-direction: column;
    align-items: center;
}
.no_data_img {
    width: 402rpx;
    height: 214rpx;
}
.no_data_text {
    font-size: 26rpx;
    
    font-weight: 400;
    color: #666666;
    /* margin-bottom: 160rpx; */
    margin-top: 8rpx;
}