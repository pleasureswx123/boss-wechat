<!-- components/timePicker/index.wxml -->
<view>
  <picker disabled="{{disabled}}" mode="multiSelector" bindchange="pickerChange" bindcolumnchange="pickerColumnChange"
    bindcancel="pickerCancel" value="{{pickerIndex}}" range="{{pickerArray}}" range-key="{{'name'}}" style="display:flex;">
    <!-- <view> -->
      <input cursor-color="#FE0201" placeholder="{{placeholder}}" class="weui-input" disabled="{{true}}" value="{{dateString}}" style="border: none;text-align: left;padding: 0;"></input>
    <!-- </view> -->
  </picker>
</view>
