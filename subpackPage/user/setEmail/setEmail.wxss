.top_input {
    padding-top: 90rpx;
    margin-left: 50%;
    transform: translateX(-50%);
    width: 652rpx;
    margin-bottom: 28rpx;
}
.notice {
    width: 652rpx;
    margin-left: 50%;
    transform: translateX(-50%);
    display: flex;
    justify-content: space-between;
    align-items: center;
    background: #F8D6D6;
    border-radius: 6rpx;
    font-size: 24rpx;
    
    font-weight: 400;
    color: #FE0201;
    padding: 14rpx 22rpx;
}
.bot_btn {
    position: fixed;
    bottom: 100rpx;
    left: 50%;
    transform: translateX(-50%);
    width: 654rpx;
    height: 80rpx;
    background: linear-gradient(270deg, #FF7979 0%, #F32E2E 100%);
    border-radius: 12rpx;
    font-size: 32rpx;
    font-family: PingFangSC-Medium, PingFang SC;
    font-weight: 500;
    color: #FFFFFF;
    line-height: 80rpx;
    text-align: center;
}
.no_data {
    background: linear-gradient(270deg, #ECECEC 0%, #D9D9D9 100%);
    border-radius: 12rpx;
    border: none;
    overflow: hidden;
    font-size: 32rpx;
    font-family: PingFangSC-Medium, PingFang SC;
    font-weight: 500;
    color: #FFFFFF;
}