page {
  background-color: #ffffff;
  overflow: hidden;
}

.item {
  height: 180rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1px solid #f5f5f5;
  margin: 0 48rpx;
}

image {
  width: 25rpx;
  height: 30rpx;
}

text {
  font-size: 28rpx;

  font-weight: 400;
  color: #666666;
}

.item view {
  flex: 1;
  text-align: right;
  margin-right: 16rpx;
  font-size: 32rpx;
  font-weight: 400;
  color: #1D2129;
}

.btn {
  height: 80rpx;
  border-radius: 12rpx;
  /* border: 1rpx solid #FFE7E7; */
  /* margin: 0 32rpx; */
  /* width: 90%; */
  font-size: 32rpx;
  font-weight: 400;
  line-height: 80rpx;
  /* color: #fff; */
  /* background: #FFFFFF linear-gradient(270deg, #FF7979 0%, #F32E2E 100%); */
  border: 1rpx solid #FFFFFF;
  height: 80rpx;
  background: #F5F5F5;
  color: #999999;
}

.Phone_footer-btn{
  width: 100%;
  position: fixed;
  bottom: 100rpx;
  padding: 0 48rpx;
  box-sizing: border-box;
}