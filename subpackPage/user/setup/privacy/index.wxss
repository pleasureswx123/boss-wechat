/* subpackPage/user/setup/privacy/index.wxss */
page {
  /* background-color: #ffffff; */
  background: #f5f5f5;
  overflow: hidden;
  width: 100%;
  height: 100%;
}

.privacy_Box {
  height: 100%;
  padding: 0rpx 48rpx;
  box-sizing: border-box;
}

.item_Box {
  padding-top: 60rpx;
}

.head {
  font-size: 28rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 600;
  color: #1D2129;
  margin-bottom: 20rpx;
}

.content {
  padding: 32rpx;
  background-color: #fff;
  border-radius: 12rpx;
  margin-bottom: 28rpx;
  display: flex;
}

.img_box {
  background-color: #FFEFEF;
  padding: 32rpx 26rpx;
  border-radius: 8rpx;
  display: flex;
  justify-content: center;
}

.img_box2 {
  height: 116rpx !important;
  width: 116rpx !important;
  display: flex;
  justify-content: center;
  padding:0;
  align-items: center;
  flex-shrink: 0;
  /* box-sizing: border-box;
  padding: 38rpx 24rpx 30rpx 30rpx !important; */
}

.big_img1 {
  width: 62rpx;
  height: 54rpx;
}

.big_img2 {
  width: 62rpx;
  height: 48rpx;
}
.big_img3 {
  width: 64rpx;
  height: 64rpx;
}

.go_img {
  width: 30rpx;
  height: 20rpx;
}

.right {
  margin-left: 32rpx;
  display: flex;
  justify-content: space-between;
  align-items: baseline;
}

.rig_text text {
  font-size: 32rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 600;
  color: #1D2129;
  margin-bottom: 14rpx;
}

.rig_text view {
  font-size: 22rpx;
  margin-top: 14rpx;
  font-weight: 400;
  color: #666666;
}

.top {
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.top .top_left {
  font-family: PingFangSC, PingFang SC;
  font-weight: 500;
  font-size: 28rpx;
  color: #1D2129;
  text-align: left;
  font-style: normal;
}

.top .top_right {
  font-family: PingFangSC, PingFang SC;
  font-weight: 500;
  font-size: 24rpx;
  color: #999999;
  line-height: 34rpx;
  text-align: left;
  font-style: normal;
}

.contactWay {
  width: 100%;
  height: 318rpx;
  background: #FFFFFF;
  border-radius: 12rpx;
  margin-top: 20rpx;
  display: flex;
  flex-direction: column;
}

.contactWay_item {
  height: 104rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 32rpx;
}

.contactWay_itemText {
  font-family: PingFangSC, PingFang SC;
  font-weight: 400;
  font-size: 28rpx;
  color: #1D2129;
  text-align: left;
  font-style: normal;
}

.contactWay_itemSelectImg image{
  width: 42rpx;
  height: 30rpx;
}