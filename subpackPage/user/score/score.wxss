.pf_1 {
  width: 62rpx;
  height: 26rpx;
  animation: swing 1.5s infinite;
}

.pf_2 {
  width: 36rpx;
  height: 36rpx;
  position: absolute;
  left: 15rpx;
  top: 0rpx
}

@keyframes swing {
  0% {
    transform: scale(1);
    opacity: 0.2;
  }

  50% {
    transform: scale(1.25);
    opacity: 0.5;
  }

  100% {
    transform: scale(1);
    opacity: 1;
  }
}

.pf_3 {
  width: 74rpx;
  height: 96rpx;
  position: absolute;
  right: 35rpx;
  top: 40rpx;
  animation: swing1 1.5s infinite;
}

@keyframes swing1 {
  0% {
    transform: none;
    top: 10px;
  }

  70% {
    top: 26px;
    transform: scale3d(0.8, 0.8, 0.8);
  }

  100% {
    top: 10px;
    transform: none;
  }
}

.modal {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  z-index: 999;
}

.modal-overlay {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.5);
}

.modal-content {
  animation: modal-show 0.3s ease-in-out;
  /* 其他样式属性 */
}

@keyframes modal-show {
  from {
    opacity: 0;
    transform: scale(0.8);
  }

  to {
    opacity: 1;
    transform: scale(1);
  }
}


