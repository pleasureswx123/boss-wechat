page {
  height: 100%;
  width: 100%;
  overflow: hidden;
}

.resumeAnnexBox {
  height: 100%;
  width: 100%;
  display: flex;
  flex-direction: column;
}

.nav-bar {
  position: fixed;
  width: 100%;
  top: 0;
  color: #fff;
}

.nav-bar .search {
  width: 100%;
  color: #333;
  font-size: 14px;
  position: absolute;
  display: flex;
  align-items: center;
  /* padding-left: 14px; */
}

.nav-bar .search .nav-bar_title{
  position: absolute;
  top: 0;
  left: 50%;
  transform: translateX(-50%);
  font-size: 36rpx;
  font-family: PingFangSC-Semibold, PingFang SC;
  font-weight: 600;
  color: #000;
}

.resumeAnnexBox-content{
  flex: 1;
  height: 100%;
  width: 100%;
  overflow: scroll;
}

.resumeAnnexBox-content .content_swiperBox{
  width: 100%;
  height: 100%;
}

.resumeAnnexBox-footer{
  height: 178rpx;
}



.in_box {
  height: 100%;
  padding: 0 47rpx;
  box-sizing: border-box;
}

.item {
  padding: 36rpx 0 30rpx 0;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.left {
  display: flex;
  align-items: center;
}

.left_img {
  width: 54rpx;
  height: 62rpx;
  margin-right: 30rpx;
}

.left_info {
  width: 500rpx;
}

.info_top {
  width: 500rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.right {
  padding: 20rpx;
  display: flex;
  align-items: center;
  margin-left: -8rpx;
}

.dian {
  width: 7rpx;
  height: 7rpx;
  border-radius: 50%;
  overflow: hidden;
  background-color: #000;
  margin-left: 8rpx;
}

.info_top {
  font-size: 28rpx;
  font-family: PingFangSC-Semibold, PingFang SC;
  font-weight: 600;
  color: #1D2129;
}

.info_bot {
  width: 500rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  font-size: 24rpx;

  font-weight: 400;
  color: #999999;
}

.set_name {
  width: 750rpx;
  height: 101rpx;
  background: #FFFFFF;
  /* border-radius: 16rpx; */
  text-align: center;
  line-height: 101rpx;
  font-size: 32rpx;

  font-weight: 400;
  color: #1D2129;
}

.set_name:first-child {
  border-bottom: 1px solid #f5f5f5;
}
.middle_over{
    color: #FE0201;
    margin: 2rpx 0;
}

.bot_over {
  /* position: fixed; */
