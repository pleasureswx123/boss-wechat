}

.footerBtn {
  padding: 0 36rpx;
  position: fixed;
  bottom: 88rpx;
  left: 50%;
  transform: translateX(-50%);
  width: 678rpx;
  height: 80rpx;
}

.bot_btn {
  width: 100%;
  box-sizing: border-box;
  height: 80rpx;
  /* background: linear-gradient(270deg, #FF7979 0%, #F32E2E 100%); */
  background: linear-gradient(308deg, #FF7C44 0%, #FE0000 46%, #FF6E67 100%);
