.logo{
    display: flex;
    align-items: center;
    justify-content: center;
    flex-direction: column;
}
page{
    background-color: #ffffff;
    overflow: hidden;
}
.logo image{
    width: 144rpx;
    height: 144rpx;
    margin-top: 75rpx;
}

.logo text{
    margin-top: 20rpx;
    margin-bottom: 80rpx;
    font-size: 24rpx;
    color: #1D2129;
}
.cell{
    margin: 0 24rpx;
}
.van-cell{
    background-color: #ffffff !important;
}
.line{
    height: 2rpx;
    background-color: #ededed;
    width: 93%;
    box-sizing: border-box;
}
.titleStyle{
    font-size: 28rpx;
    
    font-weight: 400;
    color: #1D2129;
}

.titleBox{
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.newTagBox{
  width: 50rpx;
  height: 100%;
  display: flex;
  justify-content: center;
}


.newTag{
  width: 16rpx;
  height: 16rpx;
  background: #F32E2E;
  border-radius: 50%;
}