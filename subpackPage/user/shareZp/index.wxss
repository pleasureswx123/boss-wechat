/* 定义动画关键帧 缩放 */
@keyframes clickAnimation {

  5%,
  50% {
    transform: scale(0.85);
  }

  15% {
    transform: scale(0.95);
  }

  20% {
    transform: scale(0.95) rotate(-1deg);
  }

  25% {
    transform: scale(0.95) rotate(1deg);
  }

  30% {
    transform: scale(0.95) rotate(-2deg);
  }

  35% {
    transform: scale(0.95) rotate(0deg);
  }

  40% {
    transform: scale(0.95) rotate(0);
  }
}

