@keyframes moveUpDown {

  0%,
  100% {
    transform: translateY(0);
  }

  50% {
    transform: translateY(-20rpx);
  }
}

@keyframes moveUpDown1 {

  0%,
  100% {
    transform: translateY(0);
  }

  50% {
    transform: translateY(20rpx);
  }
}

