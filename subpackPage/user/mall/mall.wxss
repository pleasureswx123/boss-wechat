/* 我的道具 */
.myProp {
  margin-left: 60rpx;
  padding: 0 44rpx;
  box-sizing: border-box;
  width: 632rpx;
  min-height: 214rpx;
  background: #fff;
  /* background: linear-gradient(84deg, rgba(255, 255, 255, 0.42) 0%, rgba(255, 255, 255, 0.26) 100%); */
  border-radius: 40rpx;
  /* border: 2rpx solid; */
  border-image: linear-gradient(142deg, rgba(255, 255, 255, 0.6), rgba(255, 137, 137, 1)) 2 2;
  backdrop-filter: blur(3px);
  margin-top: 24rpx;
}

.myPropTitle {
  padding-top: 30rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.myPropTitle view:nth-child(1) {
  font-size: 34rpx;
  font-weight: 600;
  color: #1D2129;
  line-height: 80rpx;
}

.myPropTitle view:nth-child(2) {
  font-size: 24rpx;
  font-weight: 400;
  color: #666666;
}

.myPropdetail {
  padding: 24rpx 0;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.myPropdetail_useNum {
  display: flex;
  flex-direction: column;
  font-weight: 400;

}

.myPropdetail_useNum .num {
  height: 66rpx;
  font-size: 48rpx;
  color: #1D2129;
  line-height: 66rpx;
}

.myPropdetail_useNum .degree {
  height: 40rpx;
  font-size: 28rpx;
  color: #666666;
  line-height: 40rpx;
}

.lookMyProp {
  /* width: 144rpx; */
  height: 60rpx;
  background: linear-gradient(84deg, rgba(255, 255, 255, 0.42) 0%, rgba(255, 255, 255, 0.26) 100%);
  border-radius: 40rpx;
  border-image: linear-gradient(142deg, rgba(255, 255, 255, 0.6), rgba(255, 137, 137, 0.4)) 2 2;
  backdrop-filter: blur(3px);
}

.lookMyProp .text {
  display: inline-block;
  /* padding: 14rpx 24rpx; */
  font-size: 24rpx;
  font-weight: 400;
  color: #fff;
  line-height: 60rpx;
  width: 136rpx;
  height: 48rpx;
  text-align: center;
  line-height: 48rpx;
  /* background: #D8D8D8 linear-gradient(270deg, #FF7979 0%, #F32E2E 100%); */
  background: linear-gradient(308deg, #FF7C44 0%, #FE0000 46%, #FF6E67 100%);
  border-radius: 40rpx;
}

.gcYouDou {
  padding: 0 44rpx;
  box-sizing: border-box;
  width: 632rpx;
  /* height: 444rpx; */
  background: #fff;
  /* background: linear-gradient(84deg, rgba(255, 255, 255, 0.42) 0%, rgba(255, 255, 255, 0.26) 100%); */
  border-radius: 24rpx;
  border-image: linear-gradient(142deg, rgba(255, 255, 255, 0.6), rgba(255, 210, 214, 1)) 2 2;
  backdrop-filter: blur(3px);
  padding-bottom: 2rpx;
  margin: 10rpx auto;
  position: relative;
}

.djtjBox {
  width: 632rpx;
  min-height: 372rpx;
  background: linear-gradient(270deg, #FF7979 0%, #F32E2E 100%);
  border-radius: 40rpx;
  backdrop-filter: blur(3px);
  margin: 80rpx auto 24rpx;
  position: relative;
}

.djtjBox .light {
  content: '';
  position: absolute;
  top: 0rpx;
  width: 120rpx;
  height: 100rpx;
  background-image: url('https://imgcdn.guochuanyoupin.com/resource/wechat/light.png');
  background-repeat: no-repeat;
  background-size: cover;
  /* 过度实现部分 */
  /* transition:left 0.8s; */
  animation: cross 3s infinite linear;
  opacity: 0.5;
}

@keyframes cross {
  from {
    left: -20%;
  }

  to {
    /* top: 50rpx; */
    left: 90%;
  }
}

