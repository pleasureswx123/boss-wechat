  border-radius: 40rpx;
  font-weight: 500;
  font-size: 24rpx;
  color: #FFFFFF;
  text-align: center;
  line-height: 64rpx;
}

.contentScrollView-PropContent {
  max-height: 668rpx;
  background: #FFFFFF;
  border-radius: 16rpx;
  display: flex;
  flex-direction: column;
  position: relative;
  overflow: hidden;
  padding: 32rpx 48rpx;
  margin-bottom: 28rpx;
}

.contentScrollView-PropContent .tabs-head {
  display: flex;
  align-items: center;
}

.contentScrollView-PropContent .tabs-head .tabs-head_item {
  font-weight: 400;
  font-size: 32rpx;
  color: #1D2129;
  margin-right: 32rpx;
  margin-bottom: 14rpx;
}

.contentScrollView-PropContent .tabs-head .active {
  font-weight: 600;
  position: relative;
}

.contentScrollView-PropContent .tabs-head .active::after {
  position: absolute;
  bottom: -8rpx;
  width: 44rpx;
  height: 6rpx;
  background: linear-gradient(308deg, #FF7C44 0%, #FE0000 46%, #FF6E67 100%);
  border-radius: 6rpx;
  content: "";
  left: 50%;
  transform: translateX(-50%);
}

.contentScrollView-PropContent .tab-main {
  flex: 1;
  overflow: auto;
}

.contentScrollView-PropContent .tab-main .list {
  display: flex;
  flex-direction: column;
  padding-top: 24rpx;
  overflow: hidden;
}

.contentScrollView-PropContent .tab-main .list .item {
  height: 58rpx;
  background: #F8F5F5;
  border-radius: 12rpx;
  margin-bottom: 20rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 24rpx;
}

.tab-main .list .item .item-left {
  font-weight: 400;
  font-size: 24rpx;
  color: #333333;
  display: flex;
  align-items: center;
  /* line-height: 58rpx; */
}

.tab-main .list .item .item-left image {
  width: 28rpx;
  height: 36rpx;
  margin-right: 12rpx;
}
.propIcon1{
  width: 32rpx !important;
  height: 32rpx !important;
}

.tab-main .list .item .item-right,
.tab-main .list .item .item-right_line {
