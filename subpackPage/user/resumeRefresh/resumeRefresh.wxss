}

.nav-bar {
  position: fixed;
  width: 100%;
  top: 0;
  color: #fff;
  height: 642rpx;
  background-size: cover;
  background-repeat: no-repeat;
}

.nav-bar .search {
  color: #333;
  font-size: 14px;
  position: absolute;
  display: flex;
}

.nav-bar_title {
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  font-weight: 600;
  font-size: 36rpx;
  color: #FFFFFF;
}



.resumeRefreshBox {
  height: 100%;
  width: 100;
  background: linear-gradient(180deg, #FF8271 0%, #FD432B 21%, #FFFFFF 100%);
  filter: blur(0px);
  display: flex;
  flex-direction: column;
}

.resumeRefreshBox-content {
  flex: 1;
  overflow: auto;
  padding: 0 34rpx;
  box-sizing: border-box;
}

.contentScrollView {
  height: 100%;
}

.contentScrollView-head {
  height: 180rpx;
  background: linear-gradient(180deg, #FFDFDE 0%, #FFFBF4 33%, #FFFFFF 100%, #FFE6E6 100%);
  border-radius: 16rpx;
  backdrop-filter: blur(26px);
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 40rpx;
  box-sizing: border-box;
  margin-bottom: 28rpx;
}

.contentScrollView-head .head_left {
  display: flex;
  flex-direction: column;
}

.renovateing{
  width: 34rpx;
  height: 32rpx;
  margin-left: 4rpx;
  animation: rotate360 2s linear infinite;
}

@keyframes rotate360 {
  0% {
    transform: rotate(0deg);
  }

  100% {
    transform: rotate(360deg);
  }
}

.contentScrollView-head .head_left view {
  font-weight: 600;
  font-size: 40rpx;
  color: #333333;
}

.contentScrollView-head .head_left text {
  font-weight: 400;
  font-size: 28rpx;
  color: #8F8D8D;
}

.contentScrollView-head .head_right {
  width: 146rpx;
  height: 64rpx;
