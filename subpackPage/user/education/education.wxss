.in_box {
    padding: 0 48rpx;
}

.box {
    background: #ffffff;
}

.item {
    padding: 44rpx 0;
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.items {
    padding: 44rpx 0;
    box-sizing: border-box;
}
.contnew{
  width: 600rpx;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.item_left_top {
    font-size: 24rpx;
    font-family: PingFangSC-Light, PingFang SC;
    font-weight: 300;
    color: #1D2129;
    margin-bottom: 8rpx;
}

.item_left_bot {
    font-size: 28rpx;
    font-family: PingFangSC-Medium, PingFang SC;
    font-weight: 500;
    color: #1D2129;
}

.time_box {
    display: flex;
    align-items: center;
}

.time_left {
    width: 280rpx;
    font-size: 28rpx;
    font-family: PingFangSC-Medium, PingFang SC;
    font-weight: 500;
    color: #C8C8C8;
}

.time_middle {
    margin-right: 32rpx;
}

.bot_swich {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 44rpx 0 60rpx 0;
    font-size: 32rpx;
    
    font-weight: 400;
    color: #1D2129;
}

.grey {
    color: #999999;
    
    font-weight: 400 !important;
}

.editBtn {
    display: flex;
    align-items: center;
    position: fixed;
    bottom: 70rpx;
    justify-content: center;
    width: 100%;
}

.editBtn .submit_btn1 {
    width: 456rpx;
    height: 80rpx;
    background: linear-gradient(270deg, #FF7979 0%, #F32E2E 100%);
    border-radius: 12rpx;
    text-align: center;
    line-height: 80rpx;
    font-size: 36rpx;
    
    font-weight: 400;
    color: #FFFFFF;
}

.editBtn .delete_btn {
    width: 180rpx;
    height: 80rpx;
    background: #F1F1F1;
    border-radius: 12rpx;
    text-align: center;
    line-height: 80rpx;
    font-size: 36rpx;
    
    font-weight: 400;
    color: #666666;
    margin-right: 18px;
}