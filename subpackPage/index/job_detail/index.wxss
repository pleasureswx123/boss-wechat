.yd {
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.7);
  position: absolute;
  top: 0;
  left: 0;
}

.yd image {
  width: 70%;
  height: 300rpx;
  position: absolute;
  top: 25%;
  margin-left: 15%;
}

.handImg {
  position: absolute;
  width: 102px !important;
  height: 104px !important;
  top: 40% !important;
  left: 150px;
  animation: hover 1.5s infinite;
}

@keyframes hover {
  0% {
    transform: translateY(0) translateX(0);
  }

  50% {
    transform: translateY(10px) translateX(5px);
  }

  100% {
    transform: translateY(0) translateX(0);
  }
}

.star {
  width: 60px;
  height: 60px;
  position: absolute;
  top: 40%;
  left: 60%;
  opacity: 0;
  animation: animate 0.5s linear;
}

@keyframes animate {
  0% {
    opacity: 1;
    transform: scale(1.5) translateX(1px) translateY(-1px);
  }

  20% {
    opacity: 0.9;
    transform: scale(1.4) translateX(5px) translateY(-3px);
  }

  40% {
    opacity: 0.8;
    transform: scale(1.3) translateX(8px) translateY(-5px);
  }

  60% {
    opacity: 0.6;
    transform: scale(1.2) translateX(12px) translateY(-10px);
  }

  80% {
    opacity: 0.4;
    transform: scale(1.1) translateX(15px) translateY(-16px);
  }

  100% {
    opacity: 0;
    transform: scale(1) translateX(17px) translateY(-20px);
  }
}

