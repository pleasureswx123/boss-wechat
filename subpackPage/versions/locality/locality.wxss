page {
  height: 100%;
  width: 100%;
  overflow: hidden;
  background: #F5F5F5;
}

.nav-bar {
  position: fixed;
  width: 100%;
  top: 0;
  color: #fff;
  height: 908rpx;
  background-repeat: no-repeat;
  background-size: cover;
}

.nav-bar .search {
  color: #1D2129;
  position: absolute;
  display: flex;
  align-items: center;
}

.nav-bar .search .tit {
  position: absolute;
  left: calc(50%);
  transform: translateX(-50%);
  font-weight: 600;
  font-size: 36rpx;
  color: #333333;
}

.locality_box {
  height: 100%;
  display: flex;
  flex-direction: column;
  background-repeat: no-repeat;
  background-size: 100% 300rpx;
}

.locality_box-content {
  flex: 1;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  overflow: auto;
  /* 兼容iOS 11.0-11.2 */
  padding-bottom: constant(safe-area-inset-bottom);
  /* 兼容iOS 11.2及以上 */
  padding-bottom: env(safe-area-inset-bottom);
}

.locality_box-advertisement {
  width: 436rpx;
  height: 350rpx;
  background: #FFFFFF;
  border-radius: 16rpx;
  margin-bottom: 20rpx;
  display: flex;
  flex-direction: column;
  /* border: 1rpx solid #fff; */
  margin-top: 30rpx;
  /* box-shadow: 0rpx 4rpx 4rpx 0rpx rgba(146, 53, 53, 0.1); */
  margin: 0 26rpx;
  padding-bottom: 10rpx;
  flex-shrink: 0;
}

.advertisementTop {
  height: 76rpx;
  /* background: linear-gradient(122deg, #FFE3E3 0%, #FFFAF1 18%, #FFFCFE 100%); */
  /* background: linear-gradient(121deg, #FF9E9E 0%, #FFF3DF 18%, #FFFCFE 100%); */
  border-radius: 16rpx 16rpx 0rpx 0rpx;
  backdrop-filter: blur(10px);
  border-right: 0;
  backdrop-filter: blur(10px);
  display: flex;
  align-items: center;
  justify-content: space-between;
  /* padding: 0 36rpx; */
  line-height: 76rpx;
}

.advertisementTop .left {
  height: 100%;
  display: flex;
  align-items: center;
}

.advertisementTop .left text {
  font-family: JLinXin;
  font-size: 32rpx;
  color: #1D2129;
  text-align: right;
  font-style: normal;
  background: linear-gradient(180deg, #FF7878 0%, #F74343 100%);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
}

.swiperBox {
  flex: 1;
  position: relative;
  width: 413rpx;
  height: 278rpx;
  background: #FFFFFF;
  border-radius: 22rpx;
  margin: 0 auto;
}

.swiper {
  height: 100%;
}

.swiper .swiper_item .advertising_list {
  display: grid;
  grid-template-columns: repeat(2, 1fr);
  /* 创建2列，每列宽度相等 */
  padding: 0 14rpx;
  box-sizing: border-box;
  gap: 28rpx;
  /* 设置列与列之间的间距，可以根据需求调整 */
  /* height: calc(100% - 60rpx); */
  margin-top: 40rpx;
}

.advertising_item1 {
  width: 168rpx;
  height: 56rpx;
  display: flex;
  flex-direction: row;
}

.advertising_item {
  /*width: 168rpx;
  height: 180rpx;
  position: relative;
  border-radius: 8rpx;
   border: 2rpx solid #E1E1E1; */
  display: flex;
  flex-direction: row;
  align-items: center;
  flex-wrap: wrap;
  justify-content: center;
}

.logoImage {
  width: 56rpx;
  height: 56rpx;
}

.advertising_item .labelimg {
  width: 52rpx;
  height: 62rpx;
  position: absolute;
  top: -8rpx;
  left: 12rpx;
}

.advertisingName {
  font-weight: 500;
  font-size: 22rpx;
  color: #1D2129;
  width: 100%;
  text-align: left;
  padding-left: 15rpx;
  height: 50rpx;
  line-height: 50rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.dots {
  position: absolute;
  bottom: 22rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  width: 100%;
}

.dots view {
  margin: 0 6rpx;
  border-radius: 2rpx;
  opacity: 0.7;
  width: 6rpx;
  height: 6rpx;
  background: #FEC28B;
  border-radius: 50%;
}

.dots .active {
  /* background-color: orange; */
  /* background-color: #FE0201;
  opacity: 1; */
  width: 50rpx;
  height: 6rpx;
  background: #FEC28B;
  border-radius: 4rpx;
}

.locality_box-scroll {
  flex: 1;
  display: flex;
  flex-direction: column;
  overflow: hidden;
  margin: 0 32rpx;
  margin-top: 50rpx;
}

.tabs-active {
  background: linear-gradient(308deg, #FF7C44 0%, #FE0000 46%, #FF6E67 100%);
  /* background: linear-gradient(90deg, #F43232 0%, #FF7575 100%); */
  border-radius: 6rpx 26rpx 26rpx 6rpx;
  font-weight: 400 !important;
  font-size: 32rpx !important;
  color: #FFFFFF !important;
}

.tabs {
  display: flex;
  align-items: center;
  margin-top: 20rpx;
  padding-bottom: 26rpx;
}

.tabs-item {
  width: 182rpx;
  height: 52rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  /* padding: 4rpx 28rpx; */
  font-weight: 500;
  font-size: 26rpx;
  color: #1D2129;
  font-style: normal;
  margin-right: 20rpx;
}

.tabsMain {
  /* height: 300px; */
  /* background: chartreuse; */
  display: flex;
}

.tabsMain-item {
  width: 100%;
  height: 300px;
  background: chocolate;
}

.tabs_content {
  overflow: hidden;
  /* overflow: scroll; */
  flex: 1;
}

.tabs_track {
  position: relative;
  /* display: -webkit-box;
  display: -webkit-flex; */
  display: flex;
  width: 100%;
  height: 100%;
  will-change: left;
  /* transition-duration: 0.3s; */
  /* transform: translate3d(0%, 0px, 0px); */
}

.tab_pane-wrapper {
  flex-shrink: 0;
  box-sizing: border-box;
  width: 100%;
  /* overflow: scroll; */
}

.tab_pane-wrapper-inactive {
  height: 0;
  overflow: visible;
}

.tab_pane {
  height: 100%;
  /* padding: 24px 20px;
  background-color: #F43232; */
}

.tab_pane .videoList {
  /* height: 100%; */
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}

.tab_pane .videoList .videoItem {
  position: relative;
  width: 336rpx;
  background: #fff;
  margin-bottom: 24rpx;
  border-radius: 15rpx;
}

.tab_pane .videoList .videoItem image {
  border-radius: 16rpx 16rpx 0 0;
  height: 212rpx;
}

.videoItem-image-mask {
  position: absolute;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, 0.3);
  z-index: 10;
  width: 100%;
  height: 212rpx;
  border-radius: 16rpx;
  z-index: 1;
}

.playIcon {
  width: 64rpx;
  height: 64rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  z-index: 20;
  transform: translateX(-50%) translateY(-50%);
}

.videoItem-text {
  height: 40rpx;
  font-family: PingFangSC, PingFang SC;
  font-weight: 400;
  font-size: 24rpx;
  color: #1D2129;
  line-height: 40rpx;
  text-align: left;
  font-style: normal;
  padding-top: 10rpx;
  padding-bottom: 18rpx;
  display: flex;
  justify-content: space-between;
  padding-left: 6rpx;
  padding-right: 6rpx;
}

.notData_box {
  height: 100%;
  position: relative;
}

.notData_box .notData_box_img {
  position: absolute;
  top: 40%;
  left: 50%;
