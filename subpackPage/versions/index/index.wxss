.fireworks1 {
  animation: fadeInOut 3s infinite; /* 总动画时间为 2.4s * 2 */
}

.fireworks2 {
  animation: fadeInOut 3s infinite 1.5s; /* 延迟 2.4s，总动画时间为 4.8s */
}

@keyframes fadeInOut {
  0% {
    opacity: 0; /* 初始状态：完全透明 */
  }
  20% {
    opacity: 0.35; /* 渐入：逐渐显示 */
  }
  30% {
    opacity: 0.65; /* 渐入：逐渐显示 */
  }
  40% {
    opacity: 1; /* 渐入：逐渐显示 */
  }
  50% {
    opacity: 1; /* 保持显示状态 */
  }
  60% {
    opacity: 0.65; /* 保持显示状态 */
  }
  70% {
    opacity: 0.35; /* 渐出：逐渐消失 */
  }
  80% {
    opacity: 0; /* 渐出：逐渐消失 */
  }
  100% {
    opacity: 0; /* 最终状态：完全透明 */
  }
}

