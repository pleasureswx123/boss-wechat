}

.member_openBtn {
    /* width: 648rpx; */
    height: 88rpx;
    background: linear-gradient(270deg, #822222 0%, #4B0F0F 100%);
    border-radius: 74rpx;
    margin-bottom: 24rpx;
    position: relative;
}



.buyPrice {
    display: inline-block;
    height: 100%;
    width: calc(100% - 312rpx);
    line-height: 88rpx;
    font-size: 24rpx;
    
    font-weight: 400;
    color: #FFFFFF;
    text-align: center;
}

.buyPrice text {
    font-size: 44rpx;
}

.payBtn {
    position: absolute;
    right: 40rpx;
