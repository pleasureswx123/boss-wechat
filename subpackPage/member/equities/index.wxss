/* subpackPage/member/equities/index.wxss */
page {
    height: 100%;
    overflow: hidden;
    background: #f9ddbe;
}

.member_equitiesBox {
    min-height: 100vh;
}

.member_equitiesBox_content {
    height: 100vh;
}

/* 自定义导航栏 */
.backIcon {
    background-size: cover;
    background-position: center -100%;
    width: 100vw;
    display: flex;
    position: fixed;
    top: 0;
    z-index: 999;
    align-items: center;
    padding-top: 20px;
    height: 40px;
}

.van-nav-bar__left {
    padding-left: 4px;
    align-items: center;
    display: flex;
    font-size: 14px;
    position: fixed;
    top: 0
}

.member_equities {
    height: 100%;
    width: 100%;
}

.equitiesBox {
    height: 100%;
    width: 100%;
    position: relative;
    /* display: flex; */
}

.equitiesBox .background {
    width: 100%;
    /* height: 3330rpx; */
    height: 3246rpx;
    vertical-align: bottom;
    margin-top: -10px;
}

.equitiesBox_top {
    position: absolute;
    top: 0;
    width: 100%;
}

.top_content {
    display: flex;
    position: relative;
    height: 264rpx;
}

.top_content .left {
    display: flex;
    flex-direction: column;
    position: absolute;
    padding-left: 54rpx;
    left: 0;
    z-index: 10;
}

.top_content .left .text1 {
    padding-top: 34rpx;
    font-size: 60rpx;
    /* font-family: AlibabaPuHuiTi_2_115_Black; */
    font-weight: 550;
    color: #EA0000;
    text-shadow: 0px 2px 2px rgba(193, 91, 91, 0.36);
    background: linear-gradient(96deg, #FF613F 0%, #E94042 100%);
    -webkit-background-clip: text;
    -webkit-text-fill-color: transparent;
}

.top_content .left .text2 {
    font-size: 40rpx;
    /* font-family: AlibabaPuHuiTi_2_95_ExtraBold; */
    color: #8D3411;
    font-weight: 550;
}

.top_content .right {
    width: 270rpx;
    height: 264rpx;
    position: absolute;
    right: 0;
    padding-right: 72rpx;
}

/* 主体内容 */
.equitiesBox_content {
    width: 100%;
    /* height: 2802rpx; */
    position: relative;
    padding-top: 16rpx;
}

.member_detail_box {
    width: 100%;
    height: 100%;
}

/* 权益 */
.member_equities {
    height: 100%;
    width: 100%;
}

.equities1 {
    width: 100%;
    height: 444rpx;
    text-align: center;
    position: relative;
    overflow: hidden;
}

.equities1 image {
    position: absolute;
    height: 444rpx;
    width: 654rpx;
    left: 50%;
    top:0;
    margin-left: -327rpx;
}

.equities1 .item {
    position: relative;
    width: 180rpx;
    height: 230rpx;
    padding: 10rpx 8rpx 8rpx;
    box-sizing: border-box;
    border-radius: 24rpx;
    display: inline-block;
    margin-bottom: 30rpx;
}
.identification_border{
  border:2rpx solid rgba(250, 171, 172, 0.5);
  border-radius: 24rpx;
}
.prop0_border{
  border:2rpx solid rgba(251, 163, 164, 0.5);
}
.equitiesBox{
  position: absolute;
  top:10px;
}
.equities1Box{
  position: relative;
  top:80px;
  height: 280rpx;
  overflow: hidden;
  /* left: 37px; */
  width: 622rpx;
  margin:0 auto;
}
.equities1Box .item{
  margin-right: 10px;
}
.item .title {
  height: 28rpx;
  font-size: 22rpx;
  font-weight: 500;
  color: #894D0F;
  width: 90%;
  white-space: nowrap;
  overflow: hidden;
  display: inline-block;
  text-overflow: ellipsis;
}

.equitiesText {
    display: flex;
    align-items: center;
    justify-content: center;
}

.equitiesText .equitiesline {
    width: 22rpx;
    height: 2rpx;
    opacity: 0.3;
}
.prop0 .equitiesPrice{
  text-shadow: 6rpx 0px 0px #D97E37;
}
.prop1 .equitiesPrice{
  text-shadow: 6rpx 0px 0px rgba(195,117,1,0.7);
}
.prop2 .equitiesPrice{
  text-shadow: 6rpx 0px 0px rgba(208,51,13,0.7);
}
.equitiesText text {
    font-size: 22rpx;
    
    font-weight: 400;
    color: #A75C0F;
    padding: 0 10rpx 0 8rpx;
}

.equitiesPrice {
    height: 86rpx;
    font-size: 60rpx;
    font-family: PingFangSC-Semibold, PingFang SC;
    font-weight: 600;
    color: #FFFFFF;
}

.equitiesUnit {
    font-size: 22rpx;
    font-family: PingFangSC-Semibold, PingFang SC;
    font-weight: 600;
    color: #FFFFFF;
}

.identification {
    /* left: 108rpx;
    top: 130rpx; */
    z-index: 10;
    background: linear-gradient(137deg, #FCE0C6 0%, #FFA76E 100%);
    box-shadow: 4rpx 4rpx 10rpx 0rpx rgba(227, 141, 84, 0.7);
}

.item .border {
    height: 100%;
    width: 100%;
    border: 2rpx solid transparent;
    border-radius: 24rpx;
    background-clip: padding-box, border-box;
    background-origin: padding-box, border-box;
}

.prop1 {
    /* left: 26%;
    top: 130rpx;
    transform: translateX(-50%); */
    z-index: 99;
    background: linear-gradient(136deg, #FFF0A7 0%, #F69B1A 100%);
    box-shadow: 2rpx 4rpx 12rpx 0rpx rgba(165, 108, 20, 0.52);
}

.prop1 .prop1_border {
    background-image: linear-gradient(135deg, #FFF0A7 0%, #F69B1A 100%), linear-gradient(137deg, rgba(246, 159, 32, 1), rgba(255, 238, 164, 1));
}

.prop2 {
    /* right: 108rpx;
    top: 130rpx; */
    z-index: 10;
    background: linear-gradient(135deg, #FDC7C7 0%, #FF6163 100%);
    box-shadow: 2rpx 2rpx 10rpx 0rpx rgba(206, 62, 62, 0.62);
}

.prop2 .prop2_border {
    background-image: linear-gradient(135deg, #FDC7C7 0%, #FF6163 100%), linear-gradient(137deg, rgba(255, 98, 100, 1), rgba(252, 198, 198, 1));
}

