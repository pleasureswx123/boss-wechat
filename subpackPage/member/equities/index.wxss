}

.swiperBack {
    position: absolute;
    background: #000000;
    border-radius: 38rpx;
    width: 100%;
    height: 76rpx;
    opacity: 0.5;
    top: 0;
    left: 0;
}

.swiperWarp {
    width: 100%;
    height: 76rpx;
    line-height: 76rpx;
    /* background: #000000;
    border-radius: 38rpx;
    opacity: 0.5; */
    position: relative;
    z-index: 100;
}

.swiperItem {
    display: flex;
    align-items: center;
}

.swiperItem image {
    width: 60rpx;
    height: 60rpx;
    border-radius: 50%;
    margin-right: 16rpx;
    margin-left: 16rpx;
}

.swiperItem text {
    font-size: 24rpx;
    
    font-weight: 400;
    color: #FFFFFF;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
    word-break: break-all;
    line-height: 76rpx;
}

.lucky-for {
    width: 100%;
    float: left;

}


.lucky-for>swiper-item>view {
    width: 50%;
    float: left;

}

.lucky-for>swiper-item>view:nth-child(2) {
    text-align: right;
}

.member_fliter {
    position: fixed;
    width: 648rpx;
    z-index: 100;
    padding: 40rpx 76rpx 20rpx 36rpx;
