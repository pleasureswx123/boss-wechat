}

.sitem {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.sitem image {
  width: 62rpx;
  height: 62rpx;
}

.sitem text {
  font-size: 22rpx;
  font-weight: 400;
  color: #1D2129;
}

.contBg .sCont {
  display: flex;
  overflow: auto;
  height: 100%;
  justify-content: space-around;
  padding: 0 40rpx;
}

@keyframes cross {
  from {
    left: 0%;
  }

  to {
    /* top: 50rpx; */
    left: 60%;
  }
}