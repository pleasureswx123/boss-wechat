/* subpackPage/member/memberBuy/index.wxss */
page {
  height: 100%;
  overflow: hidden;
}

.member_buyBox {
  min-height: 100vh;
  background: linear-gradient(180deg, #FEE4C5 4%, #FEE0C2 100%);
}

.member_buyBox_content {
  height: 100vh;
}

.identification_border {
  border: 2rpx solid rgba(250, 171, 172, 0.5);
  border-radius: 24rpx;
}

.prop0_border {
  border: 2rpx solid rgba(251, 163, 164, 0.5);
}

/* 自定义导航栏 */
.backIcon {
  background-size: cover;
  background-position: center -100%;
  width: 100vw;
  display: flex;
  position: fixed;
  top: 0;
  z-index: 999;
  align-items: center;
  padding-top: 20px;
  height: 40px;
}

.van-nav-bar__left {
  padding-left: 4px;
  align-items: center;
  display: flex;
  font-size: 14px;
  position: fixed;
  top: 0
}

.member_Info {
  width: 750rpx;
  height: 610rpx;
  background: linear-gradient(270deg, #2F0404 0%, #190606 100%);
}

.member_Info .memberUserInfo {
  /* width: 708rpx; */
  height: 342rpx;
  padding: 0 20rpx 0 22rpx;
  /* background-color: aqua; */
  position: relative;
}

.memberUserInfo .member_Info_back {
  width: 708rpx;
  height: 342rpx;
  position: absolute;
  z-index: 5;
}

.member_card {
  height: 100%;
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  position: relative;
  z-index: 10;
  box-sizing: border-box;
}

.card-top {
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.card-top_left {
  border-radius: 50%;
  margin-left: 46rpx;
  position: relative;
}

.card-top_left .avatar {
  width: 92rpx;
  height: 92rpx;
  border-radius: 92rpx;
}

.card-top_left .sign {
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  bottom: 10rpx;
  width: 104rpx;
  height: 24rpx;
}

.card-top_content {
  flex: 1;
  width: 0;
  padding-left: 16rpx;
}

.card-top_content .member_name {
  height: 44rpx;
  font-size: 32rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 550;
  color: #1D2129;
  line-height: 44rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.card-top_content .pastDue_time {
  height: 34rpx;
  font-size: 24rpx;

  font-weight: 400;
  color: #666666;
  line-height: 34rpx;
}

.card-top_right {
  font-size: 24rpx;

  font-weight: 400;
  color: #994009;
  position: relative;
  margin-right: 48rpx;
}

.card-top_right::before {
  position: absolute;
  content: "";
  width: 100%;
  height: 2rpx;
  top: 82%;
  background-color: #994009;
}

.card-buttom {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding-top: 36rpx;
}

.card-buttom .card-buttom_left {
  display: flex;
  flex-direction: column;
  margin-left: 46rpx;
}

.card-buttom_left .title {
  height: 34rpx;
  font-size: 24rpx;

  font-weight: 400;
  color: #1D2129;
  line-height: 34rpx;
}

.card-buttom_left .money {
  height: 60rpx;
  font-size: 44rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #1D2129;
  line-height: 60rpx;
}

.card-buttom .card-buttom_right {
  width: 216rpx;
  height: 60rpx;
  background: linear-gradient(270deg, #7B0E0E 0%, #4B0606 100%);
  border-radius: 40rpx;
  text-align: center;
  line-height: 60rpx;
  margin-right: 48rpx;
}

.card-buttom .card-buttom_right view {
  font-size: 28rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #F4D5C0;
  position: relative;
  overflow: hidden;
}

.card-buttom .card-buttom_right view::before {
  position: absolute;
  width: 170rpx;
  height: 4rpx;
  background: linear-gradient(270deg, rgba(255, 255, 255, 0) 0%, #FFFFFF 50%, rgba(255, 255, 255, 0) 100%);
  opacity: 0.72;
  content: "";
  top: 5rpx;
  left: 50%;
  transform: translateX(-50%);
}

.card-buttom .card-buttom_right view::after {
  content: '';
  position: absolute;
  top: 0rpx;
  width: 80rpx;
  height: 60rpx;
  background-image: url('https://imgcdn.guochuanyoupin.com/resource/wechat/light.png');
  background-repeat: no-repeat;
  background-size: cover;
  /* 过度实现部分 */
  /* transition:left 0.8s; */
  animation: cross 2s infinite linear;
  opacity: 0.3;
}

.member_shape {
  width: 100%;
  margin-top: -50rpx;
  position: relative;
  z-index: 0;
}

.member_shape image {
  width: 100%;
  height: 180rpx;
}

/* 主体内容 */
.member_detail {
  width: 100%;
  position: relative;
  padding-bottom: 30rpx;
}

.member_detail .member_detail_box {
  width: 100%;
  height: 100%;
}

/* 权益 */
.member_equities {
  height: 100%;
  width: 100%;
}

.equities1 {
  width: 100%;
  height: 444rpx;
  text-align: center;
  position: relative;
  overflow: hidden;
}

.equities1 image {
  position: absolute;
  height: 444rpx;
  width: 654rpx;
  left: 50%;
  top: 0;
  margin-left: -327rpx;
}

.equities1 .item {
  position: relative;
  width: 180rpx;
  height: 230rpx;
  padding: 10rpx 8rpx 8rpx;
  box-sizing: border-box;
  border-radius: 24rpx;
  display: inline-block;
}

.equitiesBox {
  position: absolute;
  top: 10px;
}

.equities1Box {
  position: relative;
  top: 80px;
  height: 230rpx;
  overflow: hidden;
  left: 37px;
  width: 622rpx;
}

.equities1Box .item {
  margin-right: 10px;
}

.item .title {
  height: 28rpx;
  font-size: 22rpx;
  font-weight: 500;
  color: #894D0F;
  width: 90%;
  white-space: nowrap;
  overflow: hidden;
  display: inline-block;
  text-overflow: ellipsis;
}

.equitiesText {
  display: flex;
  align-items: center;
  justify-content: center;
}

.equitiesText .equitiesline {
  width: 22rpx;
  height: 2rpx;
  opacity: 0.3;
}

.prop0 .equitiesPrice {
  text-shadow: 6px 0px 0px #D97E37;
}

.prop1 .equitiesPrice {
  text-shadow: 6px 0px 0px rgba(195, 117, 1, 0.7);
}

.prop2 .equitiesPrice {
  text-shadow: 6px 0px 0px rgba(208, 51, 13, 0.7);
}

.equitiesText text {
  font-size: 22rpx;

  font-weight: 400;
  color: #A75C0F;
  padding: 0 10rpx 0 8rpx;
}

.equitiesPrice {
  height: 86rpx;
  font-size: 60rpx;
  font-family: PingFangSC-Semibold, PingFang SC;
  font-weight: 600;
  color: #FFFFFF;
}

.equitiesUnit {
  font-size: 22rpx;
  font-family: PingFangSC-Semibold, PingFang SC;
  font-weight: 600;
  color: #FFFFFF;
}

.identification {
  /* left: 108rpx;
  top: 130rpx; */
  z-index: 10;
  background: linear-gradient(137deg, #FCE0C6 0%, #FFA76E 100%);
  box-shadow: 4rpx 4rpx 10rpx 0rpx rgba(227, 141, 84, 0.7);
}

.item .border {
  height: 100%;
  width: 100%;
  border: 2rpx solid transparent;
  border-radius: 24rpx;
  background-clip: padding-box, border-box;
  background-origin: padding-box, border-box;
}

.identification .identification_border {
  background-image: linear-gradient(135deg, #FCE0C6 0%, #FFA76E 100%), linear-gradient(137deg, rgba(255, 98, 100, 1), rgba(252, 198, 198, 1));
}

.prop1 {
  /* left: 26%;
  top: 130rpx;
  transform: translateX(-50%); */
  z-index: 99;
  background: linear-gradient(136deg, #FFF0A7 0%, #F69B1A 100%);
  box-shadow: 2rpx 4rpx 12rpx 0rpx rgba(165, 108, 20, 0.52);
}

.prop1 .prop1_border {
  background-image: linear-gradient(135deg, #FFF0A7 0%, #F69B1A 100%), linear-gradient(137deg, rgba(246, 159, 32, 1), rgba(255, 238, 164, 1));
}

.prop2 {
  /* right: 108rpx;
  top: 130rpx; */
  z-index: 10;
  background: linear-gradient(135deg, #FDC7C7 0%, #FF6163 100%);
  box-shadow: 2rpx 2rpx 10rpx 0rpx rgba(206, 62, 62, 0.62);
}

.prop2 .prop2_border {
  background-image: linear-gradient(135deg, #FDC7C7 0%, #FF6163 100%), linear-gradient(137deg, rgba(255, 98, 100, 1), rgba(252, 198, 198, 1));
}

