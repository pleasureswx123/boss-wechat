page{
  background: #f5f5f5;
}
.title{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  display: flex;
  padding: 0 32rpx;
  font-size: 32rpx;
  color: #999999;
}
.title view{
  margin-right: 30rpx;
}
.selected{
  color: #1D2129;
  font-weight: 600;
}
.cardItem{
  width: 692rpx;
  min-height: 204rpx;
  background: #FFFFFF;
  border-radius: 10rpx;
  margin:0 auto 30rpx;
  position: relative;
  padding-bottom: 8rpx;
  margin-top:5px;
}
.cardItem .name{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  padding:40rpx 40rpx 18rpx 40rpx;
  font-weight: 500;
}
.cardItem .time1{
  font-size: 26rpx;
  font-family: PingFangSC, PingFang SC;
  font-weight: 400;
  color: #1D2129;
  padding-left: 40rpx;
  margin-top: 5rpx;
}
.cardItem .time2{
  font-size: 26rpx;
  font-family: PingFangSC, PingFang SC;
  font-weight: 400;
  color: #1D2129;
  padding-left: 40rpx;
  margin-top: 5rpx;
}
.iconBox{
  position: absolute;
  width: 124rpx;
  height: 44rpx;
  line-height: 44rpx;
  border-radius: 2rpx 16rpx 0rpx 2rpx;
  right: 10rpx;
  font-size: 24rpx;
  font-weight: 500;
  color: #FFFFFF;
  text-align: center;
  top:-4px;
}
.recordImg{
  width: 100%;
  height: 100%;
  position: absolute;
  z-index: 1;
  right: -25rpx;
}
.iconFont{
  position: absolute;
  z-index: 2;
  right: -5rpx;
  font-weight: normal;
}